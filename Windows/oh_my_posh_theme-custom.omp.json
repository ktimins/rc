{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "postfix": "<#7a7a7a> \ue0b1</>",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f",
            "wsl": "\ue712",
            "wsl_separator": " on "
          },
          "leading_diamond": "\ue0c7\u2593",
          "foreground": "#26C6DA",
          "background": "#101010",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "display_default": false,
            "display_host": false,
            "host_color": "#2EEFBF",
            "postfix": "<#7a7a7a> \ue0b1</>",
            "user_color": "#77f5d6",
            "user_info_separator": ""
          },
          "foreground": "#fafafa",
          "background": "#101010",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "postfix": "<#7a7a7a> \ue0b1</>",
            "root_icon": "\uf0e7"
          },
          "foreground": "#ffff66",
          "background": "#101010",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "enable_hyperlink": false,
            "folder_icon": "<#B5B2C2>\ue5fe </>",
            "prefix": "<#f2f200> \ue5ff </>",
            "style": "full"
          },
          "foreground": "#fafafa",
          "background": "#101010",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "ahead_color": "#2EC4B6",
            "behind_color": "#8A4FFF",
            "color_background": false,
            "display_stash_count": true,
            "display_status": true,
            "display_upstream_icon": true,
            "local_changes_color": "#ffeb3b",
            "prefix": "<#7a7a7a>\ue0b1 </>",
            "staging_color": "#2FDA4E",
            "status_colors_enabled": true,
            "working_color": "#E84855"
          },
          "foreground": "#ffeb3b",
          "background": "#101010",
          "type": "git",
          "style": "diamond"
        },
        {
          "properties": {
            "postfix": "",
            "prefix": "",
            "text": ""
          },
          "trailing_diamond": "\ue0b0",
          "foreground": "#fafafa",
          "background": "#101010",
          "type": "text",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "postfix": "",
            "prefix": "",
            "text": ""
          },
          "leading_diamond": "\ue0b2",
          "foreground": "#fafafa",
          "background": "#101010",
          "type": "text",
          "style": "diamond"
        },
        {
          "properties": {
            "postfix": "<#7a7a7a> \ue0b3</>",
            "prefix": " \ue718 "
          },
          "foreground": "#6CA35E",
          "background": "#101010",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "postfix": "<#7a7a7a> \ue0b3</>",
            "prefix": " \ue235 "
          },
          "foreground": "#96E072",
          "background": "#101010",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "postfix": "<#7a7a7a> \ue0b3</>",
            "prefix": " \ue77f "
          },
          "foreground": "#3891A6",
          "background": "#101010",
          "type": "dotnet",
          "style": "diamond"
        },
        {
          "properties": {
            "postfix": "<#007ACC> \uf64f </>",
            "time_format": "15:04:05"
          },
          "trailing_diamond": "\u2593\ue0c6",
          "foreground": "#fafafa",
          "background": "#101010",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "text": "~#@>"
          },
          "foreground": "#7a7a7a",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "text": "<"
          },
          "foreground": "#7a7a7a",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "postfix": "<#fafafa> \uf252 </>",
            "style": "roundrock",
            "threshold": 500
          },
          "foreground": "#FFCE5C",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "always_numeric": true,
            "color_background": false,
            "display_exit_code": true,
            "error_color": "#E84855",
            "prefix": " \uf705 "
          },
          "foreground": "#9FD356",
          "type": "exit",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
