{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#FFEE58",
          "style": "plain",
          "type": "root"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "type": "session",
          "properties": {
              "user_info_separator": "",
              "display_host": false,
              "prefix": "",
              "postfix": ""
          }
        },
        {
          "background": "#e6e600",
          "foreground": "#00005f",
          "powerline_symbol": "",
          "properties": {
            "style": "full"
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "#2d2b28",
          "foreground": "#c4bcb9",
          "powerline_symbol": "",
          "style": "powerline",
          "type": "git"
        },
        {
          "background": "#906cff",
          "foreground": "#100e23",
          "powerline_symbol": "",
          "properties": {
            "prefix": "  "
          },
          "style": "powerline",
          "type": "python"
        },
        {
          "background": "#ff8080",
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "properties": {
            "prefix": ""
          },
          "style": "powerline",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true
}
